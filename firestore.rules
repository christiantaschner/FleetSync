
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are permissive and are meant for initial development.
    // They allow any authenticated user to read and write all data.
    // You should secure your database with more granular rules before deploying to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
